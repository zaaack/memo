{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jasmine Tests",
      "program": "${workspaceFolder}/node_modules/.bin/jasmine",
      "env": {
        "UPDATE_SNAPS": "1"
      },
      "args": [
        "--inspect-brk=9230",
        "--require=${workspaceFolder}/test/register.js",
        "${workspaceFolder}/test/**/*.test.ts"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ],
  "compounds": []
}
